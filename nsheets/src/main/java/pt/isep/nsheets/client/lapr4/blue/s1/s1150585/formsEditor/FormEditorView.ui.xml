<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client">
    <ui:style>

    </ui:style>
    <g:HTMLPanel>
        <ma:window.MaterialWindow ui:field="formEditorWindow" width="70%" title="Create Form">
            <m:MaterialPanel padding="5" textAlign="CENTER" height="50px">
                <m:MaterialIcon ui:field="addFormRowButton" iconType="ADD" tooltip="Add Row" iconColor="BLUE" waves="DEFAULT" circle="true" grid="s1" />
                <m:MaterialIcon ui:field="deleteFormRowButton" iconType="DELETE" tooltip="Delete Row" iconColor="BLUE" waves="DEFAULT" circle="true" grid="s1" />
                <m:MaterialIcon ui:field="editFormRowButton" iconType="EDIT" tooltip="Edit Row" iconColor="BLUE" waves="DEFAULT" circle="true" grid="s1" />            
                <m:MaterialIcon ui:field="playFormButton" iconType="PLAY_ARROW" tooltip="Play Form" iconColor="BLUE" waves="DEFAULT" circle="true" grid="s1" />
                <m:MaterialIcon ui:field="saveFormButton" iconType="SAVE" tooltip="Save Form" iconColor="BLUE" waves="DEFAULT" circle="true" grid="s1" />
            </m:MaterialPanel>      
       </ma:window.MaterialWindow> 
    </g:HTMLPanel>
</ui:UiBinder>