<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:cb="urn:import:gwt.material.design.addins.client.combobox">
             
             
    <ui:import field="pt.isep.nsheets.client.place.NameTokens.home"/>         
    <ui:with field='res' type='pt.isep.nsheets.client.resources.AppResources' />
	
     
    <m:MaterialPanel>
        <m:MaterialLabel text="Select Currency"></m:MaterialLabel>
        <m:MaterialRow>
            <m:MaterialColumn grid="s3">
                <cb:MaterialComboBox ui:field="comboBox1"></cb:MaterialComboBox>
            </m:MaterialColumn>
            <m:MaterialColumn grid="s3">
                <cb:MaterialComboBox ui:field="comboBox2"></cb:MaterialComboBox>
            </m:MaterialColumn>
            <m:MaterialColumn grid="s3">
                <m:MaterialTextBox  ui:field="value" placeholder="Value"></m:MaterialTextBox>       
            </m:MaterialColumn >
            <m:MaterialColumn grid="s3">
                <m:MaterialButton ui:field="changeBtn" text="Change"></m:MaterialButton>
            </m:MaterialColumn>
        </m:MaterialRow>
    </m:MaterialPanel>
    
</ui:UiBinder>