<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:cb="urn:import:gwt.material.design.addins.client.combobox">
    <ui:style>

    </ui:style>
    <ma:window.MaterialWindow ui:field="searchWindow" width="70%" title="Search the workbook" textAlign="LEFT" padding="32"
                              height="600px">
        <m:MaterialPanel  padding="50" textAlign="CENTER" height="100px">
            <m:MaterialNavBar ui:field="navBar" paddingLeft="20" width="100%" activates="sideBar" textColor="WHITE">
                <m:MaterialNavBrand ui:field="navBrand" text="Search"/>
                <m:MaterialLink ui:field="btnSearch" iconType="SEARCH" iconColor="WHITE" float="RIGHT"/>
            </m:MaterialNavBar>

            <!-- NavBar Search -->
            <m:MaterialNavBar ui:field="navBarSearch" width="100%" visible="false">
                <m:MaterialSearch ui:field="txtSearch" placeholder="Parameter" backgroundColor="WHITE" iconColor="BLACK"  active="true" grid="l3" shadow="1"/>
            </m:MaterialNavBar>
            <g:HTMLPanel ui:field="htmlPanel">

            </g:HTMLPanel>
        </m:MaterialPanel>

    </ma:window.MaterialWindow>
</ui:UiBinder>