<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:cb="urn:import:gwt.material.design.addins.client.combobox">
    <ui:style>

    </ui:style>
    <ma:window.MaterialWindow ui:field="formWindow" width="70%" title="Create new Chat" textAlign="LEFT" padding="32"
                              height="600px">
        <m:MaterialPanel title="Insert Chat's name">
            <m:MaterialTextBox ui:field="chatName"></m:MaterialTextBox>
        </m:MaterialPanel>
        <m:MaterialPanel title="Add member email">
            <m:MaterialTextBox ui:field="email"></m:MaterialTextBox>
            <m:MaterialButton ui:field="addMember" text="Add Member"></m:MaterialButton>
        </m:MaterialPanel>

        <m:MaterialPanel>
            <m:MaterialButton ui:field="createButton" text="Save"></m:MaterialButton>
            <m:MaterialButton ui:field="cancelButton" text="Cancel"></m:MaterialButton>
        </m:MaterialPanel>
    </ma:window.MaterialWindow>
</ui:UiBinder>